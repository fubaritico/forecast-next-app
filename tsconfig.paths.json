{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@Api": ["src/api"],
      "@Api/*": ["src/api/*"],
      "@Components/*": ["src/components/*"],
      "@Contexts/*": ["src/contexts/*"],
      "@Constants/*": ["src/constants/*"],
      "@Cypress/*": ["src/cypress/*"],
      "@Hooks/*": ["src/hooks/*"],
      "@Requests": ["src/api/requests"],
      "@Requests/*": ["src/api/requests/*"],
      "@Pages/*": ["src/pages/*"],
      "@Routes/*": ["src/routes/*"],
      "@Services/*": ["src/services/*"],
      "@Styles/*": ["src/styles/*"],
      "@Svg/*": ["src/svg/*"],
      "@Test/*": ["src/test/*"],
      "@Types": ["src/types"],
      "@Types/*": ["src/types/*"],
      "@Utils": ["src/utils"],
      "@Utils/*": ["src/utils/*"]
    }
  }
}
