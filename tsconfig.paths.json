    "baseUrl": ".",
    "paths": {
      "@Components/*": ["src/components/*"],
      "@Contexts/*": ["src/contexts/*"],
      "@Constants/*": ["src/constants/*"],
      "@Cypress/*": ["cypress/*"],
      "@Hooks/*": ["src/hooks/*"],
      "@Lib/*": ["lib/*"],
      "@Pages/*": ["pages/*"],
      "@Routes/*": ["src/routes/*"],
