{
  "name": "forecast-next-app",
  "version": "0.1.0",
  "private": true,
  "keywords": [
    "react",
    "next",
    "node",
    "express",
    "weather",
    "forecast"
  ],
  "description": "Next project providing weather forecast through a panel and a detailed dashboard for any location",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint ./src --ext ts --ext tsx --ext js",
    "format": "prettier --write ./src",
    "validate": "npm run type-check && npm run lint && npm run format && npm run test",
    "commit": "git-cz",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest --verbose --setupFiles ./jest.setup.ts",
    "test:coverage": "npm run test -- --coverage --watchAll=false",
    "icons": "node ./scripts/generateIcons.mjs"
  },
  "dependencies": {
    "@googlemaps/react-wrapper": "1.1.35",
    "@next/font": "13.1.1",
    "@storybook/react": "7.0.23",
    "apexcharts": "3.37.0",
    "axios": "1.2.1",
    "clsx": "^1.2.1",
    "handlebars": "4.7.7",
    "next": "13.4.7",
    "polished": "4.2.2",
    "react": "18.2.0",
    "react-apexcharts": "1.4.0",
    "react-dom": "18.2.0",
    "react-grid-system": "^8.1.9",
    "react-resizable": "3.0.4",
    "styled-components": "6.0.0-rc.6",
    "use-deep-compare-effect": "1.8.1"
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@storybook/addon-actions": "7.0.23",
    "@storybook/addon-essentials": "7.0.23",
    "@storybook/addon-interactions": "7.0.23",
    "@storybook/addon-links": "7.0.23",
    "@storybook/addon-mdx-gfm": "^7.0.23",
    "@storybook/addon-styling": "^1.3.1",
    "@storybook/cli": "^7.0.24",
    "@storybook/nextjs": "^7.0.23",
    "@storybook/testing-library": "0.2.0",
    "@svgr/webpack": "8.0.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/css-mediaquery": "0.1.1",
    "@types/googlemaps": "3.43.3",
    "@types/node": "20.3.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/react-resizable": "3.0.3",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "9.1.2",
    "client-only": "^0.0.1",
    "commitizen": "4.2.6",
    "css-mediaquery": "0.1.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.30.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-cypress": "2.13.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.2",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-storybook": "0.6.12",
    "eslint-plugin-testing-library": "5.11.0",
    "git-cz": "4.9.0",
    "husky": "8.0.3",
    "jest-canvas-mock": "2.4.0",
    "jest-environment-jsdom": "29.3.1",
    "lint-staged": "13.1.0",
    "postcss": "^8.4.24",
    "prettier": "2.8.8",
    "server-only": "0.0.1",
    "storybook": "^7.0.23",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tsconfig-paths-jest-mapper": "1.4.0",
    "tsconfig-paths-webpack-plugin": "4.0.1",
    "typescript": "4.9.4",
    "url-loader": "4.1.1",
    "webpack": "5.88.0"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "bash -c tsc --noEmit",
      "eslint ./src --ext ts --ext tsx --ext js",
      "prettier --write ./src",
      "bash -c jest --verbose"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
