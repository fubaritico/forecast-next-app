{
  "name": "forecast-next-app",
  "version": "0.1.0",
  "private": true,
  "keywords": [
    "react",
    "next",
    "node",
    "express",
    "weather",
    "forecast"
  ],
  "description": "Next project providing weather forecast through a panel and a detailed dashboard for any location",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint ./src --ext ts --ext tsx --ext js",
    "format": "prettier --write ./src",
    "validate": "npm run type-check && npm run lint && npm run format && npm run test",
    "commit": "git-cz",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s ./public",
    "test": "jest --verbose",
    "test:coverage": "npm run test -- --coverage --watchAll=false",
    "icons": "node ./scripts/generateIcons.mjs"
  },
  "dependencies": {
    "@googlemaps/react-wrapper": "^1.1.35",
    "@next/font": "13.1.1",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "apexcharts": "^3.37.0",
    "axios": "^1.2.1",
    "handlebars": "^4.7.7",
    "next": "13.1.1",
    "polished": "^4.2.2",
    "react": "18.2.0",
    "react-apexcharts": "^1.4.0",
    "react-dom": "18.2.0",
    "react-grid-system": "^8.1.8",
    "react-resizable": "^3.0.4",
    "styled-chart": "^1.3.8",
    "styled-components": "^5.3.6",
    "use-deep-compare-effect": "^1.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@storybook/testing-library": "0.0.13",
    "@storybook/testing-react": "^1.3.0",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/css-mediaquery": "^0.1.1",
    "@types/googlemaps": "^3.43.3",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/react-resizable": "^3.0.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "babel-loader": "^8.3.0",
    "commitizen": "^4.2.6",
    "css-mediaquery": "^0.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.30.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.8.7",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-testing-library": "5.6.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths-jest-mapper": "^1.4.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "4.9.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "bash -c tsc --noEmit",
      "eslint ./src --ext ts --ext tsx --ext js",
      "prettier --write ./src",
      "bash -c jest --verbose"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
